{
	"name": "sahne-js",
	"version": "1.0.0-rc.4",
	"description": "A tool designed for testing and debugging that uses Puppeteer's interceptor to proxy the desired requests to an internal development server from any specified URL.",
	"keywords": [
		"sahne",
		"sahne-js",
		"puppeteer",
		"debug",
		"test"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/kerematam/sahne-js"
	},
	"main": "./src/index.mjs",
	"types": "./src/index.d.ts",
	"peerDependencies": {
		"puppeteer": "^20.0.0 || ^21.0.0 || ^22.0.0"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"bin": {
		"sahne": "./cli.js"
	},
	"author": "Kerem ATAM",
	"license": "ISC",
	"dependencies": {
		"@microsoft/tsdoc": "^0.14.2",
		"@web/config-loader": "^0.3.1",
		"commander": "^11.1.0",
		"node-fetch": "^3.3.2"
	},
	"exports": {
		".": {
			"types": "./src/index.d.ts",
			"import": "./src/index.mjs",
			"require": "./src/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"cli.js",
		"src"
	]
}
